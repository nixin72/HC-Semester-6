@model IEnumerable<RAC.RACModels.CourseProfileViewModel>
<div class="panel-group">
    <div class="panel panel-default">
        <div class="panel-heading">
            <h4 class="panel-title">
                <a data-toggle="collapse" href="#ProfRes">Course Profiles</a>
            </h4>
        </div>
        <div id="ProfRes" class="panel-collapse collapse">
            <div class="panel-body"><table class="table table-responsive">
                <tr>
                    <th>
                        @Html.DisplayNameFor(model => model.CourseProfileClaraId)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.CourseProfileCode)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.ProfileName)
                    </th>

                </tr>

                @foreach (var item in Model) {
                <tr>
                    <td>
                        <input type="radio" name="CourseProfileId" value='@item.CourseProfileClaraId'>
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.CourseProfileCode)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.ProfileName)
                    </td>

                </tr>
                }

            </table></div>
        </div>
    </div>
</div>


